<% include header %>

        <div class="col-md-6 mt-6 card">
            <form class="card-body" action="/news/actulizarN" method="post">
                <h3 class="card-title">Actualizar Noticia</h3>
                <div class="form-group">
                    <input name="id_news" type="text" class="form-control" value="<%= actualiza %>" readonly>
                </div>
                <div class="form-group">
                    <input name="title" type="text" class="form-control" placeholder="Titulo">
                </div>
                <div class="form-group">
                    <input name="news" type="text" class="form-control" placeholder="Descripcion">
                </div>
               
                <input type="submit" class="btn btn-primary" value="Actualizar">
            </form>
        </div>

    <% include footer %>